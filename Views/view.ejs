<%- include('partials/header') %>

<div class="container x1">
  <div class="row">
    <div class="col-4">
      <img
        class="viewposter"
        src="<%= data.Poster %>"
        alt="<%= data.Title %>"
      />
    </div>
    <div class="col-8">
      <h1><%= data.Title %></h1>
      <p>Year:<%= data.Year %></p>
      <p>Genre:<%= data.Genre %></p>
      <p>Plot:<%= data.Plot %></p>
      <p>Rating:<%= data.imdbRating %></p>
    </div>
  </div>
</div>
<%- include('partials/footer') %>
